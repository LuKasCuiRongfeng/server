{
    "name": "pro",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "start": "concurrently -n=render,main -c=green,blue \"npm run dev\" \"npm run dev:ele\"",
        "debug": "concurrently -n=render,main -c=green,blue \"npm run dev\" \"npm run dev:debug\"",
        "dev": "webpack serve -c webpack.config.dev.ts",
        "dev:debug": "npm run build:main && wait-on tcp:12345 && electron ./dist/main/main.js --inspect=5858",
        "dev:ele": "npm run build:main && wait-on tcp:12345 &&  electron ./dist/main/main.js",
        "build:main": "rimraf dist/main && webpack -c webpack.config.main.ts",
        "build:render": "rimraf dist/render && webpack -c webpack.config.prod.ts",
        "build": "npm run build:render & npm run build:main"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.8.6",
        "@reduxjs/toolkit": "^1.8.3",
        "classnames": "^2.3.1",
        "electron": "^19.0.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "redux": "^4.2.0"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.17.12",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "css-loader": "^6.7.1",
        "less": "^4.1.3",
        "less-loader": "^11.0.0",
        "style-loader": "^3.3.1"
    }
}
